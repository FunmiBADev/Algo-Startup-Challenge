{
  "name": "carebox-pack-poc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0",
    "npm": ">=9.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently -k -n FRONTEND,BACKEND -c auto \"npm run dev\" \"npm run backend:dev\"",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "replit-start": "concurrently -k -n FRONTEND,BACKEND -c auto \"npm run dev\" \"npm run backend:dev\"",
    "replit-start:full": "concurrently -k -n FRONTEND,BACKEND -c auto \"npm run dev\" \"npm run backend:dev\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "@blockshake/defly-connect": "^1.2.1",
    "@perawallet/connect": "^1.4.1",
    "@txnlab/use-wallet": "^4.0.0",
    "@txnlab/use-wallet-react": "^4.0.0",
    "algosdk": "^3.0.0",
    "framer-motion": "^12.23.24",
    "js-sha512": "^0.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.24",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "overrides": {
    "ws@>7.0.0 <7.5.9": "7.5.10"
  }
}

